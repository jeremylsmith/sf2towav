\par \tab \tab \tab <shdr-ck>\tab ; The Sample Headers
\par \tab \tab \tab \}
\par \tab \tab )
\par 
\par 
\par }\pard\plain \s2\sb240\sa60\keepn\nowidctlpar\widctlpar\outlinelevel1\adjustright \b\f1\cgrid {{\*\bkmkstart _Toc388439273}4.3 SoundFont 2 RIFF File Format Level 2{\*\bkmkend _Toc388439273}
\par }\pard\plain \nowidctlpar\widctlpar\adjustright \f4\cgrid {\f0 
\par }\pard \nowidctlpar\widctlpar\tx1260\tx1620\tx5040\adjustright {\f0 <ifil-ck>\tab ->\tab ifil(<iver-rec>)\tab ; e.g. 2.01
\par <isng-ck>\tab ->\tab isng(szSoundEngine:ZSTR)\tab ; e.g. \ldblquote EMU8000\rdblquote 
